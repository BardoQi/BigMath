{
    "name": "bardoqi/bigmath",
    "description": "A PHP library to work with big integers and big decimals in most easy way.",
    "type": "library",
    "keywords": [
        "phpmath",
        "bigmath",
        "biginteger",
        "bigdecimal",
        "integer",
        "math",
        "php7"
    ],
    "homepage": "https://github.com/bardoqi/bigmath",
    "require": {
        "php": "~7.0",
        "ext-gmp": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "squizlabs/php_codesniffer": "^2.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bardo Qi",
            "email": "bardoqi@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev"
    "autoload": {
        "psr-4": {
            "bardoqi/bigmath/": "src",
            "files":[
                "bardoqi/bigmath/src/funcBInt.php",
                "bardoqi/bigmath/src/funcBDec.php"
            ]

        }
    },
    "autoload-dev": {
        "psr-4": {
            "bardoqi/bigmath/tests/": "tests",
            "files":[
                "bardoqi/bigmath/src/funcBInt.php",
                "bardoqi/bigmath/src/funcBDec.php"
            ]
        }
    },
}
